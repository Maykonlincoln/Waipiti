#!"/home/marquez/Desktop/Untitled Folder/wapiti/wapiti3/bin/python"
# EASY-INSTALL-ENTRY-SCRIPT: 'wapiti3==3.0.4','console_scripts','wapiti'
__requires__ = 'wapiti3==3.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wapiti3==3.0.4', 'console_scripts', 'wapiti')()
    )
